{
  "version": "1.3.0",
  "name": "@nico-i/cv-data",
  "type": "module",
  "main": "./out/index.js",
  "types": "./out/@types/index.d.ts",
  "files": [
    "out"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "bun build . --outdir ./out && bun run build:types",
    "build:types": "bunx tsc --declaration --emitDeclarationOnly ./index.ts --outDir ./out/@types",
    "gen:gql": "bunx graphql-codegen",
    "gen:gql:watch": "bunx graphql-codegen --watch",
    "o:gql": "dotenvx run -- npx opn-cli $STRAPI_URL/graphql",
    "o:cms": "dotenvx run -- npx opn-cli $STRAPI_URL/admin"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^0.27.1",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.6",
    "@graphql-codegen/typescript-generic-sdk": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.2.0",
    "@parcel/watcher": "^2.4.1",
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1"
  }
}
